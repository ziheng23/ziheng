无重复字符的最长子串
class Solution {
public:
    int lengthOfLongestSubstring(string s) {
        int re;
        int r;
        string fin;
        for(int i=0 ; i<s.length(); i++)
        {
            for(int j=i ; j<s.length();j++)
            {
                if(fin.find(s[j])==string::npos)
                {
                    fin.push_back(s[j]);   
                    r= fin.length(); 
                }
                else 
                    break;
  
            }
            fin="";
            if(re<r)
                re=r;       
        }
        return re;
    }
};
